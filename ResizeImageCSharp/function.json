{
  "disabled": false,
  "bindings": [
    {
      "path": "sample-images/{name}",
      "type": "blobTrigger",
      "name": "image",
      "direction": "in",
      "connection": "AzureWebJobsStorage"
    },
    {
      "path": "sample-images-sm/sm-{name}",
      "type": "blob",
      "name": "imageSmall",
      "direction": "out",
      "connection": "AzureWebJobsStorage"
    },
    {
      "path": "sample-images-md/md-{name}",
      "type": "blob",
      "name": "imageMedium",
      "direction": "out",
      "connection": "AzureWebJobsStorage"
    }
  ]
}